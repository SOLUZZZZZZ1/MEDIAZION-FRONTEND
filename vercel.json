{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.mediazion.eu" }],
      "destination": "https://mediazion.eu/:1",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/:1" },
    { "source": "/((?!api/).*)(?<!\\.[a-zA-Z0-9]{2,5})", "destination": "/index.html" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
