{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    { "source": "https://www.mediazion.eu/(.*)", "destination": "https://mediazion.eu/$1", "statusCode": 301 }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/index.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    }
  ]
}
