{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.mediazion.eu" }],
      "destination": "https://mediazion.eu/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/:path*", "destination": "/index.html" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}